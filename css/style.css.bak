    background-size: cover;    
    background-attachment: scroll;

}

.section-category-prod{
	background:#333;
	border-bottom: 3px solid #222;
    border-bottom-left-radius: 5px;
    border-bottom-right-radius: 5px;   
    margin-bottom: 10px;
    overflow: hidden;
    padding: 0px 5px 5px;
    position: relative;
}
.section-category-prod h3{
    color: #48a3d0;
