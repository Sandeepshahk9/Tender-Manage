body{
	margin:0px;
	font-family: 'Poppins', sans-serif;
}

.top-header-sec .navbar{
	background:#0e1123;
	padding: 5px 0px;
	border:none;
	box-shadow: 0 3px 5px rgba(16,15,15,0.16);
	border-bottom: 1px solid #444;
}

.top-header-sec{
	background:#0e1123;
	padding: 5px 0px;
	box-shadow: 0 3px 5px rgba(16,15,15,0.16);
	border-bottom: 1px solid #444;
}
.top-header-sec ul{
	list-style:none;
	margin:0px;
	padding:0px;
}
.top-header-sec ul li{
	display:inline-block;
	list-style: none;
    border-right: solid 1px #6d7083;
    position: relative;
	    padding: 0px 10px;
}
.top-header-sec ul li:last-child{	
    border-right:none;
	    padding-right: 0px;
}
.top-header-sec ul li a{
	color: #aaadbf;
    font-size: 13px;
    line-height: 0px;
}
.top-header-sec ul li a:hover{
	color: #fff;
   text-decoration:none;
}
.new-sec-heder-top{
	background-image: url(../images/main-bg.jpg);
