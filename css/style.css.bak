/*-----footer-sec*/
.footer-sec{
	background:#0e1123;  
    background-position: center;
    background-repeat: no-repeat;
    background-size: cover;
	padding:60px 0px 20px 0px;
}
.footer-sec h3{
	font-family: 'Dosis', sans-serif;
    font-size: 18px;
    color: #ffffff;
    font-weight: bold;
    text-transform: capitalize;
    margin-bottom: 30px;
}
.footer-sec ul{
	list-style:none;
	padding:0px;
}
.footer-sec ul li a{
	color:#aaadbf;
	font-size:14px;
	font-family: 'Nunito', sans-serif;
}
.footer-sec ul li a:hover{
	color: #fff;
	text-decoration:none;
	
}
.footer-sec-1 p{
	font-family: 'Nunito', sans-serif;
    font-size: 14px;
    color: #abb7c4;
    font-weight: 300;
    text-transform: none;
    line-height: 24px !important;
	
}
.footer-sec-1 ul{	
	display:inline-block;
	margin-top: 17px;
}
.footer-sec-1 ul li{
	display:inline-block;
}
.footer-sec-1 ul li:nth-child(1):hover a{
	background:#3B5998;
	color:#fff;
	border:1px solid #9898988a;
	transform: rotateZ(3600deg);
}

.footer-sec-1 ul li:nth-child(2):hover a{
	background:#dd4b39;
	color:#fff;
	border:1px solid #9898988a;
}
.footer-sec-1 ul li:nth-child(3):hover a{
	background:#55acee;
	color:#fff;
	border:1px solid #9898988a;
}
.footer-sec-1 ul li a{
	    border: 1px solid #abb7c4;
    border-radius: 22px;
    padding: 9px;
    margin: 0px 10px;
	 transition:  0.6s ease-out;
}
.footer-sec-1 h4{
	font-family: 'Nunito', sans-serif;
    font-size: 14px;
    color: #abb7c4;
    font-weight: 300;
    text-transform: none;
    line-height: 24px !important;
}
.footer-sec-1 h4 span{
	    color: #ffffff;
    font-size: 18px;
}
.footer-sec-4 p{
	font-family: 'Nunito', sans-serif;
    font-size: 19px;
    color: #fbff50;
    font-weight: 300;
    text-transform: none;
    line-height: 24px !important;
}
.footer-sec-4 .form-control{
	background:transparent;
	border:1px solid #abb7c4;
	border-radius:0px!important;
	color:#fff;
}
.footer-sec-4 .btn-default{
	background:#233a50;
	color:#fff;
	border:1px solid #abb7c4;
	border-left:none;
	border-radius:0px;
}
.copyright-sec-combine h5{
	font-family: 'Nunito', sans-serif;
    font-size: 14px;
    color: #abb7c4;
	padding-top:20px;
	text-align:center;
    font-weight: 300;
    text-transform: none;
}