	 margin:0px;
 } 
.section-category-prod ul li{
	display:inline-block;
	padding: 12px;
    background: url(../images/striped-bg.png);
 } 
 
.section-category-prod ul li a{
	color:#fff;
 } 
 
 
.section-category-prod ul li a:hover{
	color:#b1c130;
	text-decoration:none;
 } 

