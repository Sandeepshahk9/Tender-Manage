@media screen and (max-width :1200px) and (min-width:990px){
	  .section-category-prod ul {
			list-style: none;
			margin: 0px;
			padding: 0px;
		}
	  .section-category-prod ul li {
			display: inline-block;
			padding: 10px;
			background: url(../images/striped-bg.png);
		}
	  .section-category-prod ul li a {
			color: #fff;
			font-size: 12px;
		}
		.section-category-prod h3 {
			color: #48a3d0;
			font-size: 19px;
			margin-left: 0px;
		}
		.marq-slide-up{
			height:223px;
		}
		.new-upcooming-event-sec-up ul li a h3 {
			font-size: 17px;
			margin-bottom: 5px;
			margin-top: 9px;
			color: #00abff;
		}
		.new-upcooming-event-sec-up ul li a h4 {
			font-size: 11px;
		}
		.search-tender-category h2 {			
			font-size: 20px;
			
		}
		.search-tender-category ul li a {
			font-size:10px;
		}
		.featured-tender-locations h3 {			
			font-size: 17px;			
		}
		.search-tender-category-location h2 {		
			padding: 10px;			
			font-size: 13px;			
		}
		.search-tender-category-location ul li a {
			color: #1b1b1b;
			font-weight: 500;
			letter-spacing: 0px;
			font-size: 10px;
		}
}
